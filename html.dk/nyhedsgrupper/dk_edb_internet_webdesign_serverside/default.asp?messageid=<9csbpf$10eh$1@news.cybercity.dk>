# </span><br /><span class='normal'>## </span><br /><span class='normal'> </span><br /><span class='normal'>### Set Variables </span><br /><span class='normal'> </span><br /><span class='normal'>$mainurl = '<a href='http://www.rejsebeskrivelser.dk/nyheder.shtml';'>http://www.rejsebeskrivelser.dk/nyheder.shtml';</a> # news page or </span><br /><span class='normal'>site main url </span><br /><span class='normal'>$cgi_url = '<a href='http://www.rejsebeskrivelser.dk/cgi-bin/sitenews.cgi';'>http://www.rejsebeskrivelser.dk/cgi-bin/sitenews.cgi';</a> # url of </span><br /><span class='normal'>this file </span><br /><span class='normal'> </span><br /><span class='normal'>$database = 'archive.html'; # news archive file name </span><br /><span class='normal'> </span><br /><span class='normal'> </span><br /><span class='normal'>### Passwords - please change them! </span><br /><span class='normal'> </span><br /><span class='normal'>$add_password  = 'jet04kl';    # Password for news suppliers </span><br /><span class='normal'>$edit_password = 'jet04kl';   # Password for editors </span><br /><span class='normal'> </span><br /><span class='normal'> </span><br /><span class='normal'> </span><br /><span class='normal'># Define arrays for the month of the year. </span><br /><span class='normal'> </span><br /><span class='normal'># English </span><br /><span class='normal'>@months = ('Januar','Februar','Marts','April','Maj','Juni','Juli', </span><br /><span class='normal'>           'August','September','October','November','December'); </span><br /><span class='normal'> </span><br /><span class='normal'># Russian </span><br /><span class='normal'># @months = ('&#255;&#237;&#226;&#224;&#240;&#255;','&#244;&#229;&#226;&#240;&#224;&#235;&#255;','&#236;&#224;&#240;&#242;&#224;','&#224;&#239;&#240;&#229;&#235;&#255;','&#236;&#224;&#255;','&#232;&#254;&#237;&#255;','&#232;&#254;&#235;&#255;', </span><br /><span class='normal'>#            '&#224;&#226;&#227;&#243;&#241;&#242;&#224;','&#241