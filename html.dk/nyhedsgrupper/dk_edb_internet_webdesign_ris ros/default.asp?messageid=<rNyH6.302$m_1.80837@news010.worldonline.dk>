  browserwidth  = window.innerWidth;
  browserheight = window.innerHeight;      
}

screenheight = screen.height 
screenwidth  = screen.width
document.write ('<img height="1" width="1" src="/survey.asp?bw=' + browserwidth + '&bh=' + browserheight + '&sh=' + screenheight + '&sw=' + screenwidth + '">')

