<br /><span class='normal'>			 rowCount = rowCount + 1 </span><br /><span class='normal'>    			 rs.MoveNext </span><br /><span class='normal'>    			 Loop While Not rs.EOF </span><br /><span class='normal'>    			 %> </span><br /><span class='normal'>    			 &lt;/td> </span><br /><span class='normal'>    			 &lt;td width=&quot;220&quot; style=&quot;border-style: solid; border-width: </span><br /><span class='normal'>1&quot;> </span><br /><span class='normal'>    			 &lt;/td> </span><br /><span class='normal'>    			 &lt;td width=&quot;70&quot; style=&quot;border-style: solid; border-width: </span><br /><span class='normal'>1&quot;> </span><br /><span class='normal'>                 	 &lt;p align=&quot;center&quot;> </span><br /><span class='normal'>    			 &lt;/td> </span><br /><span class='normal'>    			 &lt;td width=&quot;70&quot; style=&quot;border-style: solid; border-width: </span><br /><span class='normal'>1&quot;> </span><br /><span class='normal'>    			 &lt;/td> </span><br /><span class='normal'>  			&lt;/tr> </span><br /><span class='normal'>  			&lt;% End If %>  						 </span><br /><span class='normal'>  			&lt;% if mode = &quot;alle&quot; OR mode = &quot;vis&quot; Then %> </span><br /><span class='normal'>  			&lt;tr> </span><br /><span class='normal'>  			&lt;%  </span><br /><span class='normal'>    			if mode = &quot;alle&quot; Then </span><br /><span class='normal'>			StrSQL = &quot;SELECT * FROM brugerefor WHERE status &lt;> 'UDMELDT' </span><br /><span class='normal'>AND maillist=True Order By navn&quot; </span><br /><span class='normal'>			Set rs = Conn.Execute(strSQL) </span><br /><span class='normal'>			End if </span><br /><span class='normal'>			if mode = &quot;vis&quot; then </span><br /><span class='normal'>			StrSQL = &quot;SELECT * FROM liste&quot; &amp; Request.Form(&quot;vis&quot;) &amp; &quot; where </span><br /><span class='normal'>maillist=True Order By navn&quot; </span><br /><span class='normal'>			Set rs = Conn.Execute(strSQL) </sp